#!/usr/bin/env bash
# This script is displaying numbers from 1 to 20, in the 4, 9 and 17 iteration display an additional message
i=1
while [ $i -le 20 ]
do
	echo "$i"
	case $i in
	     	4)
			echo "bad luck from China"
			;;
		9)
			echo "bad luck from Japan"
			;;
		17)
			echo "bad luck from Italy"
			;;
		*)
			;;
	esac

	(( i++ ))
done